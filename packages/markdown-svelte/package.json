{
  "name": "@vitebook/markdown-svelte",
  "version": "0.24.0",
  "keywords": [
    "esm",
    "markdown",
    "plugin",
    "vitebook",
    "svelte"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/markdown-svelte"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/client/index.js",
    "./*": "./dist/client/*",
    "./node": "./dist/node/index.js",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/types/node/index.d.ts"
      ],
      "*": [
        "dist/types/client/*"
      ]
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "run-s clean build:client build:node build:types",
    "build:client": "tsc -p src/client/tsconfig.json",
    "build:node": "node ../../.scripts/build-node.js --external magic-string",
    "build:types": "tsc -p tsconfig.json --outDir dist/types --emitDeclarationOnly",
    "clean": "rimraf dist",
    "dev": "pnpm clean && run-p watch:client watch:node watch:types",
    "watch:client": "pnpm run build:client --watch",
    "watch:node": "pnpm run build:node --watch",
    "watch:types": "pnpm run build:types --watch"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@types/lru-cache": "^5.1.1",
    "@vitebook/markdown": "0.24.0",
    "kleur": "^4.1.4",
    "lru-cache": "^6.0.0",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@vitebook/client": "workspace:*",
    "@vitebook/core": "workspace:*",
    "markdown-it": "^12.2.0",
    "svelte": "^3.49.0"
  },
  "peerDependencies": {
    "@vitebook/client": "0.24.0",
    "@vitebook/core": "0.24.0",
    "svelte": "^3.49.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
